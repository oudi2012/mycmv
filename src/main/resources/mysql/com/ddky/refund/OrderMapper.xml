<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ddky.fms.refund.mapper.OrderMapper">

    <select id="list" resultType="com.ddky.fms.refund.model.stream.entry.Order">
        select id id,total_pay totalPay  from c_order_history order by id desc
    </select>

    <select id="find" resultType="com.ddky.fms.refund.model.stream.entry.Order">
        select id id,total_pay totalPay  from c_order_history where id=#{id}
    </select>

</mapper>